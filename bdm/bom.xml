<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<businessObjectModel xmlns="http://documentation.bonitasoft.com/bdm-xml-schema/1.0" modelVersion="1.0" productVersion="8.0.0">
    <businessObjects>
        <businessObject qualifiedName="com.company.model.Demanda">
            <fields>
                <field type="STRING" length="255" name="descricao" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="categoria" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="status" nullable="true" collection="false"/>
                <relationField type="AGGREGATION" reference="com.company.model.Solicitante" fetchType="LAZY" name="solicitante" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="equipeDesignada" nullable="true" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.company.model.Solicitante">
            <fields>
                <field type="STRING" length="255" name="nome" nullable="false" collection="false"/>
                <field type="STRING" length="255" name="telefone" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="instagram" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="status" nullable="true" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries>
                <query name="buscarPorNome" content="SELECT s &#13;&#10;FROM Solicitante s &#13;&#10;WHERE s.nome = :nome" returnType="com.company.model.Solicitante">
                    <queryParameters>
                        <queryParameter name="nome" className="java.lang.String"/>
                        <queryParameter name="telefone" className="java.lang.String"/>
                        <queryParameter name="instagram" className="java.lang.String"/>
                        <queryParameter name="status" className="java.lang.String"/>
                    </queryParameters>
                </query>
            </queries>
            <indexes/>
        </businessObject>
    </businessObjects>
</businessObjectModel>
